{
  "info": {
    "_postman_id": "api-service-collection",
    "name": "API Service - Key Management & Tracking",
    "description": "Test collection for API Key Management System with PostgreSQL",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:3001/health",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3001",
          "path": ["health"]
        },
        "description": "Check if the server is running"
      },
      "response": []
    },
    {
      "name": "API Documentation",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:3001/",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3001",
          "path": [""]
        },
        "description": "View all available endpoints"
      },
      "response": []
    },
    {
      "name": "API Keys",
      "item": [
        {
          "name": "Generate New API Key",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"total_calls\": 100,\n  \"name\": \"My Test App\"\n}"
            },
            "url": {
              "raw": "http://localhost:3001/api/keys/generate",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "keys", "generate"]
            },
            "description": "Generate a new API key with a quota of 100 calls"
          },
          "response": []
        },
        {
          "name": "Get All API Keys",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/keys?page=1&limit=10",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "keys"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
            "description": "List all API keys with pagination"
          },
          "response": []
        },
        {
          "name": "Get Specific API Key Details",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/keys/1",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "keys", "1"]
            },
            "description": "Get details of a specific API key (replace 1 with actual ID)"
          },
          "response": []
        },
        {
          "name": "Get API Key Statistics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/keys/1/stats",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "keys", "1", "stats"]
            },
            "description": "Get usage statistics for a specific API key"
          },
          "response": []
        },
        {
          "name": "Revoke API Key",
          "request": {
            "method": "PATCH",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/keys/1/revoke",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "keys", "1", "revoke"]
            },
            "description": "Disable/revoke an API key"
          },
          "response": []
        },
        {
          "name": "Reactivate API Key",
          "request": {
            "method": "PATCH",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/keys/1/reactivate",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "keys", "1", "reactivate"]
            },
            "description": "Re-enable/reactivate a revoked API key"
          },
          "response": []
        },
        {
          "name": "Delete API Key",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/keys/1",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "keys", "1"]
            },
            "description": "Permanently delete an API key"
          },
          "response": []
        }
      ]
    },
    {
      "name": "API Calls & Execution",
      "item": [
        {
          "name": "Execute API Call (Uses Quota)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "X-API-Key",
                "value": "YOUR_API_KEY_HERE",
                "description": "Replace with actual API key generated above"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"endpoint\": \"/process-data\",\n  \"action\": \"process_data\"\n}"
            },
            "url": {
              "raw": "http://localhost:3001/api/calls/execute",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "calls", "execute"]
            },
            "description": "Execute an API call that tracks quota usage. Requires valid X-API-Key header"
          },
          "response": []
        },
        {
          "name": "Get My API Call Logs",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "YOUR_API_KEY_HERE",
                "description": "Replace with actual API key"
              }
            ],
            "url": {
              "raw": "http://localhost:3001/api/calls/logs?page=1&limit=50",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "calls", "logs"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "50"
                }
              ]
            },
            "description": "View your own API call logs (requires API key)"
          },
          "response": []
        },
        {
          "name": "Admin - Get All API Calls",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/calls/admin/all-calls?page=1&limit=50",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "calls", "admin", "all-calls"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "50"
                }
              ]
            },
            "description": "Admin endpoint to view all API calls from all keys"
          },
          "response": []
        },
        {
          "name": "Admin - Get Statistics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:3001/api/calls/admin/statistics",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3001",
              "path": ["api", "calls", "admin", "statistics"]
            },
            "description": "Admin endpoint to view overall API statistics (last 24 hours)"
          },
          "response": []
        }
      ]
    }
  ]
}
